.section.touch 
     .touch__container
        .row  
            .col-lg-3.col-sm-12 
                .touch-aside 
                    .office
                        span.touch-icons.fa-solid.fa-phone
                        p.touch-text  Office         
                            a.touch-text(href='tel:+78142332211') +7(814)-233-22-11
                        p.touch-text Office         
                            a.touch-text(href='tel:+78142332211') +7(814)-233-22-11
                    .mail
                        span.touch-icons.fa-regular.fa-envelope  
                        .mail-adress     
                            a.touch-text(href='email:support@gmail.com') support@gmail.com        
                            a.touch-text(href='email:info@example.com') info@example.com
                    .touch-location 
